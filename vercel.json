{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "ignoreCommand": "[ \"$VERCEL_ENV\" != production ]",
    "github": {
        "silent": true
    }
}